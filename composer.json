{
  "name": "maxim-oleinik/blade-migrations",
  "type": "library",
  "description": "Raw SQL database migration tool for PHP apps",
  "keywords": ["migrations", "database", "db", "database migrations"],
  "license": "MIT",
  "authors": [{
    "name": "Maxim Oleinik",
    "email": "maxim.oleinik@gmail.com"
  }],

  "require": {
    "php": ">=7.0",
    "maxim-oleinik/blade-database": "^0.3.0",
    "psr/log": "~1.0",
    "symfony/finder": ">=2.0"
  },
  "require-dev": {
    "phpunit/phpunit": ">=6.0"
  },

  "autoload": {
    "psr-4": {
      "Blade\\Migrations\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Blade\\Migrations\\Test\\": "test/lib"
    }
  },

  "config": {
    "sort-packages": true
  }
}
