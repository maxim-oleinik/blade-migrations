{
  "name": "blade/migrations",
  "type": "library",
  "description": "Raw SQL database migration tool for PHP app",
  "keywords": ["migrations", "database", "db", "database migrations"],
  "license": "MIT",
  "authors": [{
    "name": "Maxim Oleinik",
    "email": "maxim.oleinik@gmail.com"
  }],
  "require": {
    "php": ">=7.0",
    "blade/database": "^0.2.0",
    "symfony/finder": "*",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },
  "repositories": {
    "blade/database": {
      "type": "git",
      "url": "https://github.com/maxim-oleinik/blade-database.git"
    }
  },
  "autoload": {
    "psr-4": {
      "Blade\\Migrations\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Blade\\Migrations\\Test\\": "test/lib"
    }
  }
}
