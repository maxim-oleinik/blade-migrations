{
  "name": "blade/migrations",
  "type": "library",
  "description": "Raw SQL migration tool for PHP apps",
  "keywords": ["migrations", "database", "db", "database migrations"],
  "license": "MIT",
  "authors": [{
    "name": "Maxim Oleinik",
    "email": "maxim.oleinik@gmail.com"
  }],

  "require": {
    "php": "7.*",
    "blade/database": "^0.2.1",
    "symfony/finder": "2.8.*|3.*",
    "psr/log": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.*"
  },

  "autoload": {
    "psr-4": {
      "Blade\\Migrations\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Blade\\Migrations\\Test\\": "test/lib"
    }
  },

  "config": {
    "sort-packages": true
  }
}
